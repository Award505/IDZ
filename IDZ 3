double k = 5.0; 
        double h = 0.1; 
        double y = 2.0; 
        double end = 3.0; 

        do
        {
            double rad = Math.Acos(y / k); 
            double gr = rad * (180 / Math.PI); 

            Console.WriteLine($"y = {y:F1} м => угол = {gr:F2}°");

            y += h; 
        }
        while (y <= end); 
