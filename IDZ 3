static void getValue(double a, double b){
    double step = Math.PI/60;
    double start = a;
    Console.WriteLine($"x\ty");
    while(start < b){
        double result = Math.Tanh(start) + 1;

        Console.WriteLine($"{start:f3}\t{result:f3}");
        start = start + step;
    }
}
getValue(Math.PI/4, Math.PI/2);
