static void Main()
    {
        double k = 5.0; // длина палки
        double h = 0.1; // шаг
        double y = 2.0; // начальное значение y
        double endY = 3.0; // конечное значение y

        do
        {
            double angleRad = Math.Acos(y / k); // угол в радианах
            double angleDeg = angleRad * (180 / Math.PI); // угол в градусах

            Console.WriteLine($"y = {y:F1} м -> угол = {angleDeg:F2}°");

            y += h; // увеличиваем y на шаг h
        }
        while (y <= endY); // условие для продолжения цикла
    }
